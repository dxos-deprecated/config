{
  "name": "@dxos/config",
  "version": "1.0.0-beta.73",
  "description": "Config utilities",
  "keywords": [],
  "license": "GPL-3.0",
  "author": "DxOS.org",
  "main": "dist/es/index.js",
  "files": [
    "dist",
    "ConfigPlugin.js"
  ],
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\" --source-maps inline",
    "build:watch": "babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\" --source-maps inline --watch",
    "clean": "del-cli dist",
    "coverage": "npm test -- --coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "semistandard 'src/**/*.js'",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest --rootDir ./src --verbose --passWithNoTests --no-cache"
  },
  "dependencies": {
    "boolean": "^3.0.1",
    "debug": "^4.1.1",
    "js-yaml": "^3.13.1",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "del-cli": "^3.0.0",
    "jest": "^24.5.0",
    "semistandard": "^14.2.0",
    "webpack": "^4.10.0"
  },
  "optionalDependencies": {
    "webpack": "^4.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": [
      "jest",
      "node",
      "browser"
    ]
  }
}
